<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/my_nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.login.ui.splash.SplashFragment"
        android:label="SplashFragment" />
    <fragment
        android:id="@+id/sensorFragment"
        android:name="com.example.login.ui.sensorList.SensorListFragment"
        android:label="SensorListFragment" >
        <action
            android:id="@+id/action_sensorFragment_to_addSensorFragment"
            app:destination="@id/addSensorFragment" />
    </fragment>
    <fragment
        android:id="@+id/addSensorFragment"
        android:name="com.example.login.ui.addSensor.AddSensorFragment"
        android:label="AddSensorFragment" >
        <action
            android:id="@+id/action_addSensorFragment_to_sensorFragment"
            app:destination="@id/sensorFragment" />
    </fragment>
</navigation>