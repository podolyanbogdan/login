<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav_graph"
    app:startDestination="@id/splashFragment">
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.login.ui.splash.SplashFragment"
        android:label="SplashFragment" >
        <action
            android:id="@+id/action_splashFragment_to_ageFragment"
            app:destination="@id/ageFragment" />
    </fragment>
    <fragment
        android:id="@+id/ageFragment"
        android:name="com.example.login.ui.age.AgeFragment"
        android:label="AgeFragment" >
        <action
            android:id="@+id/action_ageFragment_to_levelFragment"
            app:destination="@id/levelFragment" />
    </fragment>
    <fragment
        android:id="@+id/levelFragment"
        android:name="com.example.login.ui.level.LevelFragment"
        android:label="LevelFragment" >
        <action
            android:id="@+id/action_levelFragment_to_ageFragment"
            app:destination="@id/ageFragment" />
        <action
            android:id="@+id/action_levelFragment_to_characterFragment"
            app:destination="@id/characterFragment" />
    </fragment>
    <fragment
        android:id="@+id/characterFragment"
        android:name="com.example.login.ui.character.CharacterFragment"
        android:label="CharacterFragment" >
        <action
            android:id="@+id/action_characterFragment_to_levelFragment"
            app:destination="@id/levelFragment" />
        <action
            android:id="@+id/action_characterFragment_to_permisionFragment"
            app:destination="@id/permisionFragment" />
    </fragment>
    <activity
        android:id="@+id/parentActivity"
        android:name="com.example.login.ui.parentScreen.ParentActivity"
        android:label="activity_parent"
        tools:layout="@layout/activity_parent" />
    <fragment
        android:id="@+id/permisionFragment"
        android:name="com.example.login.ui.permision.PermisionFragment"
        android:label="fragment_permision"
        tools:layout="@layout/fragment_permision" >
        <action
            android:id="@+id/action_permisionFragment_to_parentActivity"
            app:destination="@id/parentActivity" />
    </fragment>
</navigation>